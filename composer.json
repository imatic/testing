{
    "name": "imatic/testing",
    "description": "Library to make testing of Symfony bundles easier",
    "keywords": ["testing", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.7|~3.0",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "~2.7|~3.0",
        "phpunit/phpunit": "~5.1"
    },
    "require-dev": {
        "pds/skeleton": "~1.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpmd/phpmd": "^2.6",
        "mamuz/php-dependency-analysis": "^1.3",
        "phpdocumentor/reflection-docblock": "4.1.1 as 3.1.1"
    },
    "bin": [
        "bin/testingbundle-init"
    ],
    "autoload": {
        "psr-4": {
            "Imatic\\Testing\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
