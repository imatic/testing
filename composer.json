{
    "name": "imatic/testing",
    "description": "Library to make testing of Symfony bundles easier",
    "keywords": ["testing", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "https://www.imatic.cz/"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.4|^4.0",
        "doctrine/doctrine-bundle": "^1.2",
        "symfony/monolog-bundle": "^3.0",
        "phpunit/phpunit": "^7.4"
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpmd/phpmd": "^2.6",
        "phpdocumentor/reflection-docblock": "4.1.1 as 3.1.1"
    },
    "bin": [
        "bin/testingbundle-init"
    ],
    "autoload": {
        "psr-4": {
            "Imatic\\Testing\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
