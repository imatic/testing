{
    "name": "imatic/testing",
    "type": "library",
    "description": "Library to make testing of Symfony bundles easier",
    "keywords": ["testing", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "https://www.imatic.cz/"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.2",
        "doctrine/doctrine-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pds/skeleton": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.15"
    },
    "suggest": {
        "doctrine/doctrine-fixtures-bundle": "To use the doctrine fixtures"
    },
    "bin": [
        "bin/testingbundle-init"
    ],
    "autoload": {
        "psr-4": {
            "Imatic\\Testing\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
