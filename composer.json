{
    "name": "imatic/testing-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to make testing of Symfony bundles easier",
    "keywords": ["testing", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "require-dev": {
        "php": ">=5.4",
        "symfony/symfony": "~2.7|~3.0",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "~2.7|~3.0",
        "phpunit/phpunit": "~5.1"
    },
    "bin": [
        "testingbundle-init"
    ],
    "autoload-dev": {
        "psr-4": {
            "Imatic\\Bundle\\TestingBundle\\": ""
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
